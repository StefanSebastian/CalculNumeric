function [ l, u, p ] = descLUP( A )
%Descompunere LUP pt matricea A
[m, n] = size(A);
P = zeros(m, n);
piv = (1:m)';
for k = 1 : m - 1 
    % alege i >= k care maximizeaza uik
    [elem, i] = max(abs(A(k :m, k)));
    
    % interschimbare Ak Ai
    if k ~= i
       A = swapRow(A, i, k);
       piv
    end
    
end

end

